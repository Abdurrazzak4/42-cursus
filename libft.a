		little_len++;
	if (little_len > len)
		return (NULL);
	i = 0;
	while (i <= len - little_len)
	{
		j = 0;
		while (little[j] != '\0' && big[i + j] == little[j])
			j++;
		if (little[j] == '\0')
			return ((char *)big + i);
		i++;
	}
	return (NULL);
}
